<!--
  Tangobot visualization & navigation stack
-->
<launch>
  <!-- velocity commands remapper -->
  <node pkg="navigation_setup" name="remap_topics" type="remap_topics" args="/navigation_velocity_smoother/raw_cmd_vel /cmd_vel"/>
  <!-- Start robot model --> 
  <include file="$(find navigation_setup)/launch/tangobot_demo_visualizer.launch"/>
  <!-- Start navstack and publish map --> 
  <include file="$(find navigation_setup)/launch/tools/navstack.launch"/>
</launch>
